{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "script.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.js",
        "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin": "*"
        
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin": "*"
        
        }
      }
    ]

  }
  